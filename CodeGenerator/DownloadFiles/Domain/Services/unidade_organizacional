Éusing hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Repository;
using hbhg.Domain.Interfaces.Service;
using hbhg.Domain.Services.Common;

namespace hbhg.Domain.Services
{
    public class hbhgService : ServiceBase<hbhg>, IhbhgService
    {
        private readonly IUNIDADE_ORGANIZACIONALRepository _unidade_organizacionalRepository;
        public AnexoService(IUNIDADE_ORGANIZACIONALRepository unidade_organizacionalRepository) : base(unidade_organizacionalRepository)
        {
            _unidade_organizacionalRepository = unidade_organizacionalRepository;
        }
    }
}