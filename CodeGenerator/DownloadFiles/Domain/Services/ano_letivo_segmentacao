Éusing hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Repository;
using hbhg.Domain.Interfaces.Service;
using hbhg.Domain.Services.Common;

namespace hbhg.Domain.Services
{
    public class hbhgService : ServiceBase<hbhg>, IhbhgService
    {
        private readonly IANO_LETIVO_SEGMENTACAORepository _ano_letivo_segmentacaoRepository;
