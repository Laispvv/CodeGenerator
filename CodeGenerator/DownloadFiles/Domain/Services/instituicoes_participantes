åusing hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Repository;
using hbhg.Domain.Interfaces.Service;
using hbhg.Domain.Services.Common;

namespace hbhg.Domain.Services
{
    public class hbhgService : ServiceBase<hbhg>, IhbhgService
    {
        private readonly IINSTITUICOES_PARTICIPANTESRepository _instituicoes_participantesRepository;
        public AnexoService(IINSTITUICOES_PARTICIPANTESRepository instituicoes_participantesRepository) : base(instituicoes_participantesRepository)
        {
            _instituicoes_participantesRepository = instituicoes_participantesRepository;
        }
    }
}