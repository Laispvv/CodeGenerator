Éusing hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Repository;
using hbhg.Domain.Interfaces.Service;
using hbhg.Domain.Services.Common;

namespace hbhg.Domain.Services
{
    public class hbhgService : ServiceBase<hbhg>, IhbhgService
    {
        private readonly IDISCIPLINA_FUNCIONARIORepository _disciplina_funcionarioRepository;
        public AnexoService(IDISCIPLINA_FUNCIONARIORepository disciplina_funcionarioRepository) : base(disciplina_funcionarioRepository)
        {
            _disciplina_funcionarioRepository = disciplina_funcionarioRepository;
        }
    }
}