Éusing hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Repository;
using hbhg.Domain.Interfaces.Service;
using hbhg.Domain.Services.Common;

namespace hbhg.Domain.Services
{
    public class hbhgService : ServiceBase<hbhg>, IhbhgService
    {
        private readonly IMATRICULA_REQUERIMENTORepository _matricula_requerimentoRepository;
        public AnexoService(IMATRICULA_REQUERIMENTORepository matricula_requerimentoRepository) : base(matricula_requerimentoRepository)
        {
            _matricula_requerimentoRepository = matricula_requerimentoRepository;
        }
    }
}