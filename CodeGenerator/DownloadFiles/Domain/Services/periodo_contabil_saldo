Éusing hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Repository;
using hbhg.Domain.Interfaces.Service;
using hbhg.Domain.Services.Common;

namespace hbhg.Domain.Services
{
    public class hbhgService : ServiceBase<hbhg>, IhbhgService
    {
        private readonly IPERIODO_CONTABIL_SALDORepository _periodo_contabil_saldoRepository;
        public AnexoService(IPERIODO_CONTABIL_SALDORepository periodo_contabil_saldoRepository) : base(periodo_contabil_saldoRepository)
        {
            _periodo_contabil_saldoRepository = periodo_contabil_saldoRepository;
        }
    }
}