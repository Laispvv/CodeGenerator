Úusing hbhg.Application.Interfaces;
using hbhg.Application.ViewModels;
using Microsoft.AspNetCore.Mvc;

namespace hbhg.Api.Controllers
{
    [Route("api/[controller]/")]
    [ApiController]
    public class CURRICULO_SERIESController : BaseController<CURRICULO_SERIESViewModel>
    {
        private readonly ICURRICULO_SERIESServiceAppService _curriculo_seriesAppService;
        public CURRICULO_SERIESController(ICURRICULO_SERIESAppService curriculo_seriesAppService) : base(curriculo_seriesAppService)
        {
            _curriculo_seriesAppService = CURRICULO_SERIESAppService;
        }
    }
}