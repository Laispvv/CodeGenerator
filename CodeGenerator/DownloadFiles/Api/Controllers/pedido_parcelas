Ðusing hbhg.Application.Interfaces;
using hbhg.Application.ViewModels;
using Microsoft.AspNetCore.Mvc;

namespace hbhg.Api.Controllers
{
    [Route("api/[controller]/")]
    [ApiController]
    public class PEDIDO_PARCELASController : BaseController<PEDIDO_PARCELASViewModel>
    {
        private readonly IPEDIDO_PARCELASServiceAppService _pedido_parcelasAppService;
        public PEDIDO_PARCELASController(IPEDIDO_PARCELASAppService pedido_parcelasAppService) : base(pedido_parcelasAppService)
        {
            _pedido_parcelasAppService = PEDIDO_PARCELASAppService;
        }
    }
}