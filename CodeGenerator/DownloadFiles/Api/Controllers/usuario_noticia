Ðusing hbhg.Application.Interfaces;
using hbhg.Application.ViewModels;
using Microsoft.AspNetCore.Mvc;

namespace hbhg.Api.Controllers
{
    [Route("api/[controller]/")]
    [ApiController]
    public class USUARIO_NOTICIAController : BaseController<USUARIO_NOTICIAViewModel>
    {
        private readonly IUSUARIO_NOTICIAServiceAppService _usuario_noticiaAppService;
        public USUARIO_NOTICIAController(IUSUARIO_NOTICIAAppService usuario_noticiaAppService) : base(usuario_noticiaAppService)
        {
            _usuario_noticiaAppService = USUARIO_NOTICIAAppService;
        }
    }
}