using Microsoft.AspNetCore.Mvc;

namespace hbhg.Api.Controllers
{
    [Route("api/[controller]/")]
    [ApiController]
    public class CONTAS_RECEBER_DESCONTOController : BaseController<CONTAS_RECEBER_DESCONTOViewModel>
    {
