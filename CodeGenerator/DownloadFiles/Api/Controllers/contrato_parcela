Úusing hbhg.Application.Interfaces;
using hbhg.Application.ViewModels;
using Microsoft.AspNetCore.Mvc;

namespace hbhg.Api.Controllers
{
    [Route("api/[controller]/")]
    [ApiController]
    public class CONTRATO_PARCELAController : BaseController<CONTRATO_PARCELAViewModel>
    {
        private readonly ICONTRATO_PARCELAServiceAppService _contrato_parcelaAppService;
        public CONTRATO_PARCELAController(ICONTRATO_PARCELAAppService contrato_parcelaAppService) : base(contrato_parcelaAppService)
        {
            _contrato_parcelaAppService = CONTRATO_PARCELAAppService;
        }
    }
}