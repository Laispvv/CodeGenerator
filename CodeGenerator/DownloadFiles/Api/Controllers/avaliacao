using hbhg.Application.ViewModels;
using Microsoft.AspNetCore.Mvc;

namespace hbhg.Api.Controllers
{
    [Route("api/[controller]/")]
    [ApiController]
    public class AVALIACAOController : BaseController<AVALIACAOViewModel>
    {
        private readonly IAVALIACAOServiceAppService _avaliacaoAppService;
        public AVALIACAOController(IAVALIACAOAppService avaliacaoAppService) : base(avaliacaoAppService)
        {
            _avaliacaoAppService = AVALIACAOAppService;
        }
    }
}