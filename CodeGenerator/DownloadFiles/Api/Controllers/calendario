žusing hbhg.Application.Interfaces;
using hbhg.Application.ViewModels;
using Microsoft.AspNetCore.Mvc;

namespace hbhg.Api.Controllers
{
    [Route("api/[controller]/")]
    [ApiController]
    public class CALENDARIOController : BaseController<CALENDARIOViewModel>
    {
        private readonly ICALENDARIOServiceAppService _calendarioAppService;
        public CALENDARIOController(ICALENDARIOAppService calendarioAppService) : base(calendarioAppService)
        {
            _calendarioAppService = CALENDARIOAppService;
        }
    }
}