žusing hbhg.Application.Interfaces;
using hbhg.Application.ViewModels;
using Microsoft.AspNetCore.Mvc;

namespace hbhg.Api.Controllers
{
    [Route("api/[controller]/")]
    [ApiController]
    public class FORNECEDORController : BaseController<FORNECEDORViewModel>
    {
        private readonly IFORNECEDORServiceAppService _fornecedorAppService;
        public FORNECEDORController(IFORNECEDORAppService fornecedorAppService) : base(fornecedorAppService)
        {
            _fornecedorAppService = FORNECEDORAppService;
        }
    }
}