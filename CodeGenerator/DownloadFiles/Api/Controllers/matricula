using hbhg.Application.ViewModels;
using Microsoft.AspNetCore.Mvc;

namespace hbhg.Api.Controllers
{
    [Route("api/[controller]/")]
    [ApiController]
    public class MATRICULAController : BaseController<MATRICULAViewModel>
    {
        private readonly IMATRICULAServiceAppService _matriculaAppService;
        public MATRICULAController(IMATRICULAAppService matriculaAppService) : base(matriculaAppService)
        {
            _matriculaAppService = MATRICULAAppService;
        }
    }
}