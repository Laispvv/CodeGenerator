žusing hbhg.Application.Interfaces;
using hbhg.Application.ViewModels;
using Microsoft.AspNetCore.Mvc;

namespace hbhg.Api.Controllers
{
    [Route("api/[controller]/")]
    [ApiController]
    public class AREA_CURSOController : BaseController<AREA_CURSOViewModel>
    {
        private readonly IAREA_CURSOServiceAppService _area_cursoAppService;
        public AREA_CURSOController(IAREA_CURSOAppService area_cursoAppService) : base(area_cursoAppService)
        {
            _area_cursoAppService = AREA_CURSOAppService;
        }
    }
}