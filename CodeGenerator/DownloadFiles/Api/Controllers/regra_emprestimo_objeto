 using hbhg.Application.Interfaces;
using hbhg.Application.ViewModels;
using Microsoft.AspNetCore.Mvc;

namespace hbhg.Api.Controllers
{
    [Route("api/[controller]/")]
    [ApiController]
    public class REGRA_EMPRESTIMO_OBJETOController : BaseController<REGRA_EMPRESTIMO_OBJETOViewModel>
    {
        private readonly IREGRA_EMPRESTIMO_OBJETOServiceAppService _regra_emprestimo_objetoAppService;
        public REGRA_EMPRESTIMO_OBJETOController(IREGRA_EMPRESTIMO_OBJETOAppService regra_emprestimo_objetoAppService) : base(regra_emprestimo_objetoAppService)
        {
            _regra_emprestimo_objetoAppService = REGRA_EMPRESTIMO_OBJETOAppService;
        }
    }
}