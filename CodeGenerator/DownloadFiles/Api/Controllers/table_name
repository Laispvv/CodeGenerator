žusing hbhg.Application.Interfaces;
using hbhg.Application.ViewModels;
using Microsoft.AspNetCore.Mvc;

namespace hbhg.Api.Controllers
{
    [Route("api/[controller]/")]
    [ApiController]
    public class TABLE_NAMEController : BaseController<TABLE_NAMEViewModel>
    {
        private readonly ITABLE_NAMEServiceAppService _table_nameAppService;
        public TABLE_NAMEController(ITABLE_NAMEAppService table_nameAppService) : base(table_nameAppService)
        {
            _table_nameAppService = TABLE_NAMEAppService;
        }
    }
}