²using hbhg.Application.Interfaces;
using hbhg.Application.ViewModels;
using Microsoft.AspNetCore.Mvc;

namespace hbhg.Api.Controllers
{
    [Route("api/[controller]/")]
    [ApiController]
    public class FLUXO_MEDIDAController : BaseController<FLUXO_MEDIDAViewModel>
    {
        private readonly IFLUXO_MEDIDAServiceAppService _fluxo_medidaAppService;
