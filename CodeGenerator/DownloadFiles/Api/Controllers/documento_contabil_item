 using hbhg.Application.Interfaces;
using hbhg.Application.ViewModels;
using Microsoft.AspNetCore.Mvc;

namespace hbhg.Api.Controllers
{
    [Route("api/[controller]/")]
    [ApiController]
    public class DOCUMENTO_CONTABIL_ITEMController : BaseController<DOCUMENTO_CONTABIL_ITEMViewModel>
    {
        private readonly IDOCUMENTO_CONTABIL_ITEMServiceAppService _documento_contabil_itemAppService;
        public DOCUMENTO_CONTABIL_ITEMController(IDOCUMENTO_CONTABIL_ITEMAppService documento_contabil_itemAppService) : base(documento_contabil_itemAppService)
        {
            _documento_contabil_itemAppService = DOCUMENTO_CONTABIL_ITEMAppService;
        }
    }
}