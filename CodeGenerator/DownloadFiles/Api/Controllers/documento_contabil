using Microsoft.AspNetCore.Mvc;

namespace hbhg.Api.Controllers
{
    [Route("api/[controller]/")]
    [ApiController]
    public class DOCUMENTO_CONTABILController : BaseController<DOCUMENTO_CONTABILViewModel>
    {
