¼using hbhg.Application.Interfaces;
using hbhg.Application.ViewModels;
using Microsoft.AspNetCore.Mvc;

namespace hbhg.Api.Controllers
{
    [Route("api/[controller]/")]
    [ApiController]
    public class PERFIL_ACESSOController : BaseController<PERFIL_ACESSOViewModel>
    {
        private readonly IPERFIL_ACESSOServiceAppService _perfil_acessoAppService;
        public PERFIL_ACESSOController(IPERFIL_ACESSOAppService perfil_acessoAppService) : base(perfil_acessoAppService)
        {
            _perfil_acessoAppService = PERFIL_ACESSOAppService;
        }
    }
}