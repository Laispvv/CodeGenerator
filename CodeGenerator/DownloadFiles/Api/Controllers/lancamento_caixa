Úusing hbhg.Application.Interfaces;
using hbhg.Application.ViewModels;
using Microsoft.AspNetCore.Mvc;

namespace hbhg.Api.Controllers
{
    [Route("api/[controller]/")]
    [ApiController]
    public class LANCAMENTO_CAIXAController : BaseController<LANCAMENTO_CAIXAViewModel>
    {
        private readonly ILANCAMENTO_CAIXAServiceAppService _lancamento_caixaAppService;
        public LANCAMENTO_CAIXAController(ILANCAMENTO_CAIXAAppService lancamento_caixaAppService) : base(lancamento_caixaAppService)
        {
            _lancamento_caixaAppService = LANCAMENTO_CAIXAAppService;
        }
    }
}