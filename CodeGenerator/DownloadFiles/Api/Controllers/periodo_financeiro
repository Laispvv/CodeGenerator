îusing hbhg.Application.Interfaces;
using hbhg.Application.ViewModels;
using Microsoft.AspNetCore.Mvc;

namespace hbhg.Api.Controllers
{
    [Route("api/[controller]/")]
    [ApiController]
    public class PERIODO_FINANCEIROController : BaseController<PERIODO_FINANCEIROViewModel>
    {
        private readonly IPERIODO_FINANCEIROServiceAppService _periodo_financeiroAppService;
        public PERIODO_FINANCEIROController(IPERIODO_FINANCEIROAppService periodo_financeiroAppService) : base(periodo_financeiroAppService)
        {
            _periodo_financeiroAppService = PERIODO_FINANCEIROAppService;
        }
    }
}