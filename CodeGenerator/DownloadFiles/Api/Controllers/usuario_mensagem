Úusing hbhg.Application.Interfaces;
using hbhg.Application.ViewModels;
using Microsoft.AspNetCore.Mvc;

namespace hbhg.Api.Controllers
{
    [Route("api/[controller]/")]
    [ApiController]
    public class USUARIO_MENSAGEMController : BaseController<USUARIO_MENSAGEMViewModel>
    {
        private readonly IUSUARIO_MENSAGEMServiceAppService _usuario_mensagemAppService;
        public USUARIO_MENSAGEMController(IUSUARIO_MENSAGEMAppService usuario_mensagemAppService) : base(usuario_mensagemAppService)
        {
            _usuario_mensagemAppService = USUARIO_MENSAGEMAppService;
        }
    }
}