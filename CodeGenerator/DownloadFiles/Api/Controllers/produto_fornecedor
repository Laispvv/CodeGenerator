îusing hbhg.Application.Interfaces;
using hbhg.Application.ViewModels;
using Microsoft.AspNetCore.Mvc;

namespace hbhg.Api.Controllers
{
    [Route("api/[controller]/")]
    [ApiController]
    public class PRODUTO_FORNECEDORController : BaseController<PRODUTO_FORNECEDORViewModel>
    {
        private readonly IPRODUTO_FORNECEDORServiceAppService _produto_fornecedorAppService;
        public PRODUTO_FORNECEDORController(IPRODUTO_FORNECEDORAppService produto_fornecedorAppService) : base(produto_fornecedorAppService)
        {
            _produto_fornecedorAppService = PRODUTO_FORNECEDORAppService;
        }
    }
}