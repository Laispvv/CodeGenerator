Úusing hbhg.Application.Interfaces;
using hbhg.Application.ViewModels;
using Microsoft.AspNetCore.Mvc;

namespace hbhg.Api.Controllers
{
    [Route("api/[controller]/")]
    [ApiController]
    public class TURMA_DISCIPLINAController : BaseController<TURMA_DISCIPLINAViewModel>
    {
        private readonly ITURMA_DISCIPLINAServiceAppService _turma_disciplinaAppService;
        public TURMA_DISCIPLINAController(ITURMA_DISCIPLINAAppService turma_disciplinaAppService) : base(turma_disciplinaAppService)
        {
            _turma_disciplinaAppService = TURMA_DISCIPLINAAppService;
        }
    }
}