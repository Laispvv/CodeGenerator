Ðusing hbhg.Application.Interfaces;
using hbhg.Application.ViewModels;
using Microsoft.AspNetCore.Mvc;

namespace hbhg.Api.Controllers
{
    [Route("api/[controller]/")]
    [ApiController]
    public class INTEGRACAO_FICOController : BaseController<INTEGRACAO_FICOViewModel>
    {
        private readonly IINTEGRACAO_FICOServiceAppService _integracao_ficoAppService;
        public INTEGRACAO_FICOController(IINTEGRACAO_FICOAppService integracao_ficoAppService) : base(integracao_ficoAppService)
        {
            _integracao_ficoAppService = INTEGRACAO_FICOAppService;
        }
    }
}