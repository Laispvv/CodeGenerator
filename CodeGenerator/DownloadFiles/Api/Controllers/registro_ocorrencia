øusing hbhg.Application.Interfaces;
using hbhg.Application.ViewModels;
using Microsoft.AspNetCore.Mvc;

namespace hbhg.Api.Controllers
{
    [Route("api/[controller]/")]
    [ApiController]
    public class REGISTRO_OCORRENCIAController : BaseController<REGISTRO_OCORRENCIAViewModel>
    {
        private readonly IREGISTRO_OCORRENCIAServiceAppService _registro_ocorrenciaAppService;
        public REGISTRO_OCORRENCIAController(IREGISTRO_OCORRENCIAAppService registro_ocorrenciaAppService) : base(registro_ocorrenciaAppService)
        {
            _registro_ocorrenciaAppService = REGISTRO_OCORRENCIAAppService;
        }
    }
}