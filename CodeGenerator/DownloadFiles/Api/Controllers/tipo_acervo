¨using hbhg.Application.Interfaces;
using hbhg.Application.ViewModels;
using Microsoft.AspNetCore.Mvc;

namespace hbhg.Api.Controllers
{
    [Route("api/[controller]/")]
    [ApiController]
    public class TIPO_ACERVOController : BaseController<TIPO_ACERVOViewModel>
    {
        private readonly ITIPO_ACERVOServiceAppService _tipo_acervoAppService;
