îusing hbhg.Application.Interfaces;
using hbhg.Application.ViewModels;
using Microsoft.AspNetCore.Mvc;

namespace hbhg.Api.Controllers
{
    [Route("api/[controller]/")]
    [ApiController]
    public class PARECER_DESCRITIVOController : BaseController<PARECER_DESCRITIVOViewModel>
    {
        private readonly IPARECER_DESCRITIVOServiceAppService _parecer_descritivoAppService;
        public PARECER_DESCRITIVOController(IPARECER_DESCRITIVOAppService parecer_descritivoAppService) : base(parecer_descritivoAppService)
        {
            _parecer_descritivoAppService = PARECER_DESCRITIVOAppService;
        }
    }
}