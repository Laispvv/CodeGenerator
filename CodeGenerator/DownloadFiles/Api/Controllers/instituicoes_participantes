¾using hbhg.Application.Interfaces;
using hbhg.Application.ViewModels;
using Microsoft.AspNetCore.Mvc;

namespace hbhg.Api.Controllers
{
    [Route("api/[controller]/")]
    [ApiController]
    public class INSTITUICOES_PARTICIPANTESController : BaseController<INSTITUICOES_PARTICIPANTESViewModel>
    {
        private readonly IINSTITUICOES_PARTICIPANTESServiceAppService _instituicoes_participantesAppService;
