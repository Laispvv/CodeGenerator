using Microsoft.AspNetCore.Mvc;

namespace hbhg.Api.Controllers
{
    [Route("api/[controller]/")]
    [ApiController]
    public class FORMA_PAGAMENTOController : BaseController<FORMA_PAGAMENTOViewModel>
    {
        private readonly IFORMA_PAGAMENTOServiceAppService _forma_pagamentoAppService;
