žusing hbhg.Application.Interfaces;
using hbhg.Application.ViewModels;
using Microsoft.AspNetCore.Mvc;

namespace hbhg.Api.Controllers
{
    [Route("api/[controller]/")]
    [ApiController]
    public class CONVENIADOController : BaseController<CONVENIADOViewModel>
    {
        private readonly ICONVENIADOServiceAppService _conveniadoAppService;
        public CONVENIADOController(ICONVENIADOAppService conveniadoAppService) : base(conveniadoAppService)
        {
            _conveniadoAppService = CONVENIADOAppService;
        }
    }
}