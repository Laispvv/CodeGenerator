îusing hbhg.Application.Interfaces;
using hbhg.Application.ViewModels;
using Microsoft.AspNetCore.Mvc;

namespace hbhg.Api.Controllers
{
    [Route("api/[controller]/")]
    [ApiController]
    public class FREQUENCIA_OFICINAController : BaseController<FREQUENCIA_OFICINAViewModel>
    {
        private readonly IFREQUENCIA_OFICINAServiceAppService _frequencia_oficinaAppService;
        public FREQUENCIA_OFICINAController(IFREQUENCIA_OFICINAAppService frequencia_oficinaAppService) : base(frequencia_oficinaAppService)
        {
            _frequencia_oficinaAppService = FREQUENCIA_OFICINAAppService;
        }
    }
}