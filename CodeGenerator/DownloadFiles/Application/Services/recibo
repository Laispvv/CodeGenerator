þusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class RECIBOAppService : ServiceBase<RECIBOViewModel, RECIBO>, IRECIBOAppService
    {
        private readonly IMapper _mapper;
        private readonly IRECIBOService _reciboService;
        public RECIBOAppService(IMapper mapper, IRECIBOService reciboService) : base(mapper, reciboService)
        {
            _mapper = mapper;
            _reciboService = reciboService;
        }
    }
}