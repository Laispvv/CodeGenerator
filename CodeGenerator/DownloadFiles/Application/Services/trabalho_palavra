öusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class TRABALHO_PALAVRAAppService : ServiceBase<TRABALHO_PALAVRAViewModel, TRABALHO_PALAVRA>, ITRABALHO_PALAVRAAppService
    {
        private readonly IMapper _mapper;
        private readonly ITRABALHO_PALAVRAService _trabalho_palavraService;
        public TRABALHO_PALAVRAAppService(IMapper mapper, ITRABALHO_PALAVRAService trabalho_palavraService) : base(mapper, trabalho_palavraService)
        {
            _mapper = mapper;
            _trabalho_palavraService = trabalho_palavraService;
        }
    }
}