êusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class PEDIDO_PARCELASAppService : ServiceBase<PEDIDO_PARCELASViewModel, PEDIDO_PARCELAS>, IPEDIDO_PARCELASAppService
    {
        private readonly IMapper _mapper;
        private readonly IPEDIDO_PARCELASService _pedido_parcelasService;
        public PEDIDO_PARCELASAppService(IMapper mapper, IPEDIDO_PARCELASService pedido_parcelasService) : base(mapper, pedido_parcelasService)
        {
            _mapper = mapper;
            _pedido_parcelasService = pedido_parcelasService;
        }
    }
}