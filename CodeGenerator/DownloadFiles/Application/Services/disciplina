®using AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class DISCIPLINAAppService : ServiceBase<DISCIPLINAViewModel, DISCIPLINA>, IDISCIPLINAAppService
    {
        private readonly IMapper _mapper;
        private readonly IDISCIPLINAService _disciplinaService;
        public DISCIPLINAAppService(IMapper mapper, IDISCIPLINAService disciplinaService) : base(mapper, disciplinaService)
        {
            _mapper = mapper;
            _disciplinaService = disciplinaService;
        }
    }
}