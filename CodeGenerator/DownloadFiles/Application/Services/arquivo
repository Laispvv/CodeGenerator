using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class ARQUIVOAppService : ServiceBase<ARQUIVOViewModel, ARQUIVO>, IARQUIVOAppService
    {
        private readonly IMapper _mapper;
        private readonly IARQUIVOService _arquivoService;
        public ARQUIVOAppService(IMapper mapper, IARQUIVOService arquivoService) : base(mapper, arquivoService)
        {
            _mapper = mapper;
            _arquivoService = arquivoService;
        }
    }
}