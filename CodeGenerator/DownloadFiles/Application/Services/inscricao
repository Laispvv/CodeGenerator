¢using AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class INSCRICAOAppService : ServiceBase<INSCRICAOViewModel, INSCRICAO>, IINSCRICAOAppService
    {
        private readonly IMapper _mapper;
        private readonly IINSCRICAOService _inscricaoService;
        public INSCRICAOAppService(IMapper mapper, IINSCRICAOService inscricaoService) : base(mapper, inscricaoService)
        {
            _mapper = mapper;
            _inscricaoService = inscricaoService;
        }
    }
}