æusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class AULAAppService : ServiceBase<AULAViewModel, AULA>, IAULAAppService
    {
        private readonly IMapper _mapper;
        private readonly IAULAService _aulaService;
        public AULAAppService(IMapper mapper, IAULAService aulaService) : base(mapper, aulaService)
        {
            _mapper = mapper;
            _aulaService = aulaService;
        }
    }
}