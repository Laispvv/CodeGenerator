²using AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class PRECO_FORMA_PAGAMENTOAppService : ServiceBase<PRECO_FORMA_PAGAMENTOViewModel, PRECO_FORMA_PAGAMENTO>, IPRECO_FORMA_PAGAMENTOAppService
    {
        private readonly IMapper _mapper;
        private readonly IPRECO_FORMA_PAGAMENTOService _preco_forma_pagamentoService;
        public PRECO_FORMA_PAGAMENTOAppService(IMapper mapper, IPRECO_FORMA_PAGAMENTOService preco_forma_pagamentoService) : base(mapper, preco_forma_pagamentoService)
        {
            _mapper = mapper;
            _preco_forma_pagamentoService = preco_forma_pagamentoService;
        }
    }
}