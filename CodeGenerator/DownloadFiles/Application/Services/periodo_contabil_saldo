¾using AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class PERIODO_CONTABIL_SALDOAppService : ServiceBase<PERIODO_CONTABIL_SALDOViewModel, PERIODO_CONTABIL_SALDO>, IPERIODO_CONTABIL_SALDOAppService
    {
        private readonly IMapper _mapper;
        private readonly IPERIODO_CONTABIL_SALDOService _periodo_contabil_saldoService;
        public PERIODO_CONTABIL_SALDOAppService(IMapper mapper, IPERIODO_CONTABIL_SALDOService periodo_contabil_saldoService) : base(mapper, periodo_contabil_saldoService)
        {
            _mapper = mapper;
            _periodo_contabil_saldoService = periodo_contabil_saldoService;
        }
    }
}