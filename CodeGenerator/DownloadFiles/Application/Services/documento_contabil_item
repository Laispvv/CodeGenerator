{
    public class DOCUMENTO_CONTABIL_ITEMAppService : ServiceBase<DOCUMENTO_CONTABIL_ITEMViewModel, DOCUMENTO_CONTABIL_ITEM>, IDOCUMENTO_CONTABIL_ITEMAppService
