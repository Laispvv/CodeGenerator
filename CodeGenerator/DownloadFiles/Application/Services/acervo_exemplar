êusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class ACERVO_EXEMPLARAppService : ServiceBase<ACERVO_EXEMPLARViewModel, ACERVO_EXEMPLAR>, IACERVO_EXEMPLARAppService
    {
        private readonly IMapper _mapper;
        private readonly IACERVO_EXEMPLARService _acervo_exemplarService;
        public ACERVO_EXEMPLARAppService(IMapper mapper, IACERVO_EXEMPLARService acervo_exemplarService) : base(mapper, acervo_exemplarService)
        {
            _mapper = mapper;
            _acervo_exemplarService = acervo_exemplarService;
        }
    }
}