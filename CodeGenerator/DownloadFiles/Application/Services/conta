òusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class CONTAAppService : ServiceBase<CONTAViewModel, CONTA>, ICONTAAppService
    {
        private readonly IMapper _mapper;
        private readonly ICONTAService _contaService;
        public CONTAAppService(IMapper mapper, ICONTAService contaService) : base(mapper, contaService)
        {
            _mapper = mapper;
            _contaService = contaService;
        }
    }
}