using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class INSCRICAO_OFICINAAppService : ServiceBase<INSCRICAO_OFICINAViewModel, INSCRICAO_OFICINA>, IINSCRICAO_OFICINAAppService
    {
        private readonly IMapper _mapper;
        private readonly IINSCRICAO_OFICINAService _inscricao_oficinaService;
