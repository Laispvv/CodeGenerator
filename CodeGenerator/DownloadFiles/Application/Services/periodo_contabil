öusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class PERIODO_CONTABILAppService : ServiceBase<PERIODO_CONTABILViewModel, PERIODO_CONTABIL>, IPERIODO_CONTABILAppService
    {
        private readonly IMapper _mapper;
        private readonly IPERIODO_CONTABILService _periodo_contabilService;
        public PERIODO_CONTABILAppService(IMapper mapper, IPERIODO_CONTABILService periodo_contabilService) : base(mapper, periodo_contabilService)
        {
            _mapper = mapper;
            _periodo_contabilService = periodo_contabilService;
        }
    }
}