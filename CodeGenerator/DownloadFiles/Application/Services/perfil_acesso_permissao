{
    public class PERFIL_ACESSO_PERMISSAOAppService : ServiceBase<PERFIL_ACESSO_PERMISSAOViewModel, PERFIL_ACESSO_PERMISSAO>, IPERFIL_ACESSO_PERMISSAOAppService
