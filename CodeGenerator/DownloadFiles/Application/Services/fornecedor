®using AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class FORNECEDORAppService : ServiceBase<FORNECEDORViewModel, FORNECEDOR>, IFORNECEDORAppService
    {
        private readonly IMapper _mapper;
        private readonly IFORNECEDORService _fornecedorService;
        public FORNECEDORAppService(IMapper mapper, IFORNECEDORService fornecedorService) : base(mapper, fornecedorService)
        {
            _mapper = mapper;
            _fornecedorService = fornecedorService;
        }
    }
}