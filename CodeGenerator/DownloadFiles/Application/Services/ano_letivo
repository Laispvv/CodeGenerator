®using AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class ANO_LETIVOAppService : ServiceBase<ANO_LETIVOViewModel, ANO_LETIVO>, IANO_LETIVOAppService
    {
        private readonly IMapper _mapper;
        private readonly IANO_LETIVOService _ano_letivoService;
        public ANO_LETIVOAppService(IMapper mapper, IANO_LETIVOService ano_letivoService) : base(mapper, ano_letivoService)
        {
            _mapper = mapper;
            _ano_letivoService = ano_letivoService;
        }
    }
}