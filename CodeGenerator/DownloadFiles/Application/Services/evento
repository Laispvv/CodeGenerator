using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class EVENTOAppService : ServiceBase<EVENTOViewModel, EVENTO>, IEVENTOAppService
    {
        private readonly IMapper _mapper;
        private readonly IEVENTOService _eventoService;
        public EVENTOAppService(IMapper mapper, IEVENTOService eventoService) : base(mapper, eventoService)
        {
            _mapper = mapper;
            _eventoService = eventoService;
        }
    }
}