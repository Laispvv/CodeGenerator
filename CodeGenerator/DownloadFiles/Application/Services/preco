òusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class PRECOAppService : ServiceBase<PRECOViewModel, PRECO>, IPRECOAppService
    {
        private readonly IMapper _mapper;
        private readonly IPRECOService _precoService;
        public PRECOAppService(IMapper mapper, IPRECOService precoService) : base(mapper, precoService)
        {
            _mapper = mapper;
            _precoService = precoService;
        }
    }
}