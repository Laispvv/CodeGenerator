namespace hbhg.Application.Services
{
    public class PRECO_OBJETOAppService : ServiceBase<PRECO_OBJETOViewModel, PRECO_OBJETO>, IPRECO_OBJETOAppService
    {
        private readonly IMapper _mapper;
        private readonly IPRECO_OBJETOService _preco_objetoService;
