òusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class FLUXOAppService : ServiceBase<FLUXOViewModel, FLUXO>, IFLUXOAppService
    {
        private readonly IMapper _mapper;
        private readonly IFLUXOService _fluxoService;
        public FLUXOAppService(IMapper mapper, IFLUXOService fluxoService) : base(mapper, fluxoService)
        {
            _mapper = mapper;
            _fluxoService = fluxoService;
        }
    }
}