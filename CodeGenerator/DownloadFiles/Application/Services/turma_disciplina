öusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class TURMA_DISCIPLINAAppService : ServiceBase<TURMA_DISCIPLINAViewModel, TURMA_DISCIPLINA>, ITURMA_DISCIPLINAAppService
    {
        private readonly IMapper _mapper;
        private readonly ITURMA_DISCIPLINAService _turma_disciplinaService;
        public TURMA_DISCIPLINAAppService(IMapper mapper, ITURMA_DISCIPLINAService turma_disciplinaService) : base(mapper, turma_disciplinaService)
        {
            _mapper = mapper;
            _turma_disciplinaService = turma_disciplinaService;
        }
    }
}