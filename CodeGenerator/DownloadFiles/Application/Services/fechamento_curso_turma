¾using AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class FECHAMENTO_CURSO_TURMAAppService : ServiceBase<FECHAMENTO_CURSO_TURMAViewModel, FECHAMENTO_CURSO_TURMA>, IFECHAMENTO_CURSO_TURMAAppService
    {
        private readonly IMapper _mapper;
        private readonly IFECHAMENTO_CURSO_TURMAService _fechamento_curso_turmaService;
        public FECHAMENTO_CURSO_TURMAAppService(IMapper mapper, IFECHAMENTO_CURSO_TURMAService fechamento_curso_turmaService) : base(mapper, fechamento_curso_turmaService)
        {
            _mapper = mapper;
            _fechamento_curso_turmaService = fechamento_curso_turmaService;
        }
    }
}