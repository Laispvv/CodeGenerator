æusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class AREAAppService : ServiceBase<AREAViewModel, AREA>, IAREAAppService
    {
        private readonly IMapper _mapper;
        private readonly IAREAService _areaService;
        public AREAAppService(IMapper mapper, IAREAService areaService) : base(mapper, areaService)
        {
            _mapper = mapper;
            _areaService = areaService;
        }
    }
}