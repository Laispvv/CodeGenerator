Žusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class PERIODO_FINANCEIROAppService : ServiceBase<PERIODO_FINANCEIROViewModel, PERIODO_FINANCEIRO>, IPERIODO_FINANCEIROAppService
    {
        private readonly IMapper _mapper;
        private readonly IPERIODO_FINANCEIROService _periodo_financeiroService;
        public PERIODO_FINANCEIROAppService(IMapper mapper, IPERIODO_FINANCEIROService periodo_financeiroService) : base(mapper, periodo_financeiroService)
        {
            _mapper = mapper;
            _periodo_financeiroService = periodo_financeiroService;
        }
    }
}