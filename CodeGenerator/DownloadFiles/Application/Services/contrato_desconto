using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class CONTRATO_DESCONTOAppService : ServiceBase<CONTRATO_DESCONTOViewModel, CONTRATO_DESCONTO>, ICONTRATO_DESCONTOAppService
    {
        private readonly IMapper _mapper;
        private readonly ICONTRATO_DESCONTOService _contrato_descontoService;
