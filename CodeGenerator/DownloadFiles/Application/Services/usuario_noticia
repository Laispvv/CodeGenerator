êusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class USUARIO_NOTICIAAppService : ServiceBase<USUARIO_NOTICIAViewModel, USUARIO_NOTICIA>, IUSUARIO_NOTICIAAppService
    {
        private readonly IMapper _mapper;
        private readonly IUSUARIO_NOTICIAService _usuario_noticiaService;
        public USUARIO_NOTICIAAppService(IMapper mapper, IUSUARIO_NOTICIAService usuario_noticiaService) : base(mapper, usuario_noticiaService)
        {
            _mapper = mapper;
            _usuario_noticiaService = usuario_noticiaService;
        }
    }
}