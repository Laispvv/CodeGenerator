using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class OFICINAAppService : ServiceBase<OFICINAViewModel, OFICINA>, IOFICINAAppService
    {
        private readonly IMapper _mapper;
        private readonly IOFICINAService _oficinaService;
        public OFICINAAppService(IMapper mapper, IOFICINAService oficinaService) : base(mapper, oficinaService)
        {
            _mapper = mapper;
            _oficinaService = oficinaService;
        }
    }
}