using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class CURRICULO_ESCOLARAppService : ServiceBase<CURRICULO_ESCOLARViewModel, CURRICULO_ESCOLAR>, ICURRICULO_ESCOLARAppService
    {
        private readonly IMapper _mapper;
        private readonly ICURRICULO_ESCOLARService _curriculo_escolarService;
        public CURRICULO_ESCOLARAppService(IMapper mapper, ICURRICULO_ESCOLARService curriculo_escolarService) : base(mapper, curriculo_escolarService)
        {
            _mapper = mapper;
            _curriculo_escolarService = curriculo_escolarService;
        }
    }
}