îusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class INSTITUICOES_PARTICIPANTESAppService : ServiceBase<INSTITUICOES_PARTICIPANTESViewModel, INSTITUICOES_PARTICIPANTES>, IINSTITUICOES_PARTICIPANTESAppService
    {
        private readonly IMapper _mapper;
        private readonly IINSTITUICOES_PARTICIPANTESService _instituicoes_participantesService;
        public INSTITUICOES_PARTICIPANTESAppService(IMapper mapper, IINSTITUICOES_PARTICIPANTESService instituicoes_participantesService) : base(mapper, instituicoes_participantesService)
        {
            _mapper = mapper;
            _instituicoes_participantesService = instituicoes_participantesService;
        }
    }
}