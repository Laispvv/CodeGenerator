þusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class PEDIDOAppService : ServiceBase<PEDIDOViewModel, PEDIDO>, IPEDIDOAppService
    {
        private readonly IMapper _mapper;
        private readonly IPEDIDOService _pedidoService;
        public PEDIDOAppService(IMapper mapper, IPEDIDOService pedidoService) : base(mapper, pedidoService)
        {
            _mapper = mapper;
            _pedidoService = pedidoService;
        }
    }
}