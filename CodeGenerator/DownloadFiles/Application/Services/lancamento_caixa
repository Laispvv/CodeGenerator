öusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class LANCAMENTO_CAIXAAppService : ServiceBase<LANCAMENTO_CAIXAViewModel, LANCAMENTO_CAIXA>, ILANCAMENTO_CAIXAAppService
    {
        private readonly IMapper _mapper;
        private readonly ILANCAMENTO_CAIXAService _lancamento_caixaService;
        public LANCAMENTO_CAIXAAppService(IMapper mapper, ILANCAMENTO_CAIXAService lancamento_caixaService) : base(mapper, lancamento_caixaService)
        {
            _mapper = mapper;
            _lancamento_caixaService = lancamento_caixaService;
        }
    }
}