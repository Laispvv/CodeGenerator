¢using AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class MATRICULAAppService : ServiceBase<MATRICULAViewModel, MATRICULA>, IMATRICULAAppService
    {
        private readonly IMapper _mapper;
        private readonly IMATRICULAService _matriculaService;
        public MATRICULAAppService(IMapper mapper, IMATRICULAService matriculaService) : base(mapper, matriculaService)
        {
            _mapper = mapper;
            _matriculaService = matriculaService;
        }
    }
}