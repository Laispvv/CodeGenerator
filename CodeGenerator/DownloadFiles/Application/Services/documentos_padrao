namespace hbhg.Application.Services
{
    public class DOCUMENTOS_PADRAOAppService : ServiceBase<DOCUMENTOS_PADRAOViewModel, DOCUMENTOS_PADRAO>, IDOCUMENTOS_PADRAOAppService
    {
        private readonly IMapper _mapper;
        private readonly IDOCUMENTOS_PADRAOService _documentos_padraoService;
