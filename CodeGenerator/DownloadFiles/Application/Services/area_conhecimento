using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class AREA_CONHECIMENTOAppService : ServiceBase<AREA_CONHECIMENTOViewModel, AREA_CONHECIMENTO>, IAREA_CONHECIMENTOAppService
    {
        private readonly IMapper _mapper;
        private readonly IAREA_CONHECIMENTOService _area_conhecimentoService;
        public AREA_CONHECIMENTOAppService(IMapper mapper, IAREA_CONHECIMENTOService area_conhecimentoService) : base(mapper, area_conhecimentoService)
        {
            _mapper = mapper;
            _area_conhecimentoService = area_conhecimentoService;
        }
    }
}