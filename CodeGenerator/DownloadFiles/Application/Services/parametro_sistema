using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class PARAMETRO_SISTEMAAppService : ServiceBase<PARAMETRO_SISTEMAViewModel, PARAMETRO_SISTEMA>, IPARAMETRO_SISTEMAAppService
    {
        private readonly IMapper _mapper;
        private readonly IPARAMETRO_SISTEMAService _parametro_sistemaService;
        public PARAMETRO_SISTEMAAppService(IMapper mapper, IPARAMETRO_SISTEMAService parametro_sistemaService) : base(mapper, parametro_sistemaService)
        {
            _mapper = mapper;
            _parametro_sistemaService = parametro_sistemaService;
        }
    }
}