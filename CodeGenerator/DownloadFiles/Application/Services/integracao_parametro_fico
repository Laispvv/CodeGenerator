âusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class INTEGRACAO_PARAMETRO_FICOAppService : ServiceBase<INTEGRACAO_PARAMETRO_FICOViewModel, INTEGRACAO_PARAMETRO_FICO>, IINTEGRACAO_PARAMETRO_FICOAppService
    {
        private readonly IMapper _mapper;
        private readonly IINTEGRACAO_PARAMETRO_FICOService _integracao_parametro_ficoService;
        public INTEGRACAO_PARAMETRO_FICOAppService(IMapper mapper, IINTEGRACAO_PARAMETRO_FICOService integracao_parametro_ficoService) : base(mapper, integracao_parametro_ficoService)
        {
            _mapper = mapper;
            _integracao_parametro_ficoService = integracao_parametro_ficoService;
        }
    }
}