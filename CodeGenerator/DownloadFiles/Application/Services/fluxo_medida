using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class FLUXO_MEDIDAAppService : ServiceBase<FLUXO_MEDIDAViewModel, FLUXO_MEDIDA>, IFLUXO_MEDIDAAppService
    {
        private readonly IMapper _mapper;
        private readonly IFLUXO_MEDIDAService _fluxo_medidaService;
        public FLUXO_MEDIDAAppService(IMapper mapper, IFLUXO_MEDIDAService fluxo_medidaService) : base(mapper, fluxo_medidaService)
        {
            _mapper = mapper;
            _fluxo_medidaService = fluxo_medidaService;
        }
    }
}