²using AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class MATRICULA_RESPONSAVELAppService : ServiceBase<MATRICULA_RESPONSAVELViewModel, MATRICULA_RESPONSAVEL>, IMATRICULA_RESPONSAVELAppService
    {
        private readonly IMapper _mapper;
        private readonly IMATRICULA_RESPONSAVELService _matricula_responsavelService;
        public MATRICULA_RESPONSAVELAppService(IMapper mapper, IMATRICULA_RESPONSAVELService matricula_responsavelService) : base(mapper, matricula_responsavelService)
        {
            _mapper = mapper;
            _matricula_responsavelService = matricula_responsavelService;
        }
    }
}