using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class FAMILIAAppService : ServiceBase<FAMILIAViewModel, FAMILIA>, IFAMILIAAppService
    {
        private readonly IMapper _mapper;
        private readonly IFAMILIAService _familiaService;
        public FAMILIAAppService(IMapper mapper, IFAMILIAService familiaService) : base(mapper, familiaService)
        {
            _mapper = mapper;
            _familiaService = familiaService;
        }
    }
}