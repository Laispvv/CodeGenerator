æusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class NOTAAppService : ServiceBase<NOTAViewModel, NOTA>, INOTAAppService
    {
        private readonly IMapper _mapper;
        private readonly INOTAService _notaService;
        public NOTAAppService(IMapper mapper, INOTAService notaService) : base(mapper, notaService)
        {
            _mapper = mapper;
            _notaService = notaService;
        }
    }
}