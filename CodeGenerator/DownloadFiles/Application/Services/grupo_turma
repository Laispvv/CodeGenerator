ºusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class GRUPO_TURMAAppService : ServiceBase<GRUPO_TURMAViewModel, GRUPO_TURMA>, IGRUPO_TURMAAppService
    {
        private readonly IMapper _mapper;
        private readonly IGRUPO_TURMAService _grupo_turmaService;
        public GRUPO_TURMAAppService(IMapper mapper, IGRUPO_TURMAService grupo_turmaService) : base(mapper, grupo_turmaService)
        {
            _mapper = mapper;
            _grupo_turmaService = grupo_turmaService;
        }
    }
}