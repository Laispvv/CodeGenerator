öusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class REGRA_EMPRESTIMOAppService : ServiceBase<REGRA_EMPRESTIMOViewModel, REGRA_EMPRESTIMO>, IREGRA_EMPRESTIMOAppService
    {
        private readonly IMapper _mapper;
        private readonly IREGRA_EMPRESTIMOService _regra_emprestimoService;
        public REGRA_EMPRESTIMOAppService(IMapper mapper, IREGRA_EMPRESTIMOService regra_emprestimoService) : base(mapper, regra_emprestimoService)
        {
            _mapper = mapper;
            _regra_emprestimoService = regra_emprestimoService;
        }
    }
}