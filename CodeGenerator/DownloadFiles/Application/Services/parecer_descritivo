using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class PARECER_DESCRITIVOAppService : ServiceBase<PARECER_DESCRITIVOViewModel, PARECER_DESCRITIVO>, IPARECER_DESCRITIVOAppService
    {
        private readonly IMapper _mapper;
        private readonly IPARECER_DESCRITIVOService _parecer_descritivoService;
