using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class LISTA_MATERIALAppService : ServiceBase<LISTA_MATERIALViewModel, LISTA_MATERIAL>, ILISTA_MATERIALAppService
    {
        private readonly IMapper _mapper;
        private readonly ILISTA_MATERIALService _lista_materialService;
        public LISTA_MATERIALAppService(IMapper mapper, ILISTA_MATERIALService lista_materialService) : base(mapper, lista_materialService)
        {
            _mapper = mapper;
            _lista_materialService = lista_materialService;
        }
    }
}