ºusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class ORGANIZACAOAppService : ServiceBase<ORGANIZACAOViewModel, ORGANIZACAO>, IORGANIZACAOAppService
    {
        private readonly IMapper _mapper;
        private readonly IORGANIZACAOService _organizacaoService;
        public ORGANIZACAOAppService(IMapper mapper, IORGANIZACAOService organizacaoService) : base(mapper, organizacaoService)
        {
            _mapper = mapper;
            _organizacaoService = organizacaoService;
        }
    }
}