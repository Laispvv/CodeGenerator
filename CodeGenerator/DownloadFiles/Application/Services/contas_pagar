using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class CONTAS_PAGARAppService : ServiceBase<CONTAS_PAGARViewModel, CONTAS_PAGAR>, ICONTAS_PAGARAppService
    {
        private readonly IMapper _mapper;
        private readonly ICONTAS_PAGARService _contas_pagarService;
        public CONTAS_PAGARAppService(IMapper mapper, ICONTAS_PAGARService contas_pagarService) : base(mapper, contas_pagarService)
        {
            _mapper = mapper;
            _contas_pagarService = contas_pagarService;
        }
    }
}