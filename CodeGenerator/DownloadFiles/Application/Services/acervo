using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class ACERVOAppService : ServiceBase<ACERVOViewModel, ACERVO>, IACERVOAppService
    {
        private readonly IMapper _mapper;
        private readonly IACERVOService _acervoService;
        public ACERVOAppService(IMapper mapper, IACERVOService acervoService) : base(mapper, acervoService)
        {
            _mapper = mapper;
            _acervoService = acervoService;
        }
    }
}