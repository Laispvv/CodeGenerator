öusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class CONTRATO_PARCELAAppService : ServiceBase<CONTRATO_PARCELAViewModel, CONTRATO_PARCELA>, ICONTRATO_PARCELAAppService
    {
        private readonly IMapper _mapper;
        private readonly ICONTRATO_PARCELAService _contrato_parcelaService;
        public CONTRATO_PARCELAAppService(IMapper mapper, ICONTRATO_PARCELAService contrato_parcelaService) : base(mapper, contrato_parcelaService)
        {
            _mapper = mapper;
            _contrato_parcelaService = contrato_parcelaService;
        }
    }
}