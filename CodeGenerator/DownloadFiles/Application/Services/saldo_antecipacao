using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class SALDO_ANTECIPACAOAppService : ServiceBase<SALDO_ANTECIPACAOViewModel, SALDO_ANTECIPACAO>, ISALDO_ANTECIPACAOAppService
    {
        private readonly IMapper _mapper;
        private readonly ISALDO_ANTECIPACAOService _saldo_antecipacaoService;
        public SALDO_ANTECIPACAOAppService(IMapper mapper, ISALDO_ANTECIPACAOService saldo_antecipacaoService) : base(mapper, saldo_antecipacaoService)
        {
            _mapper = mapper;
            _saldo_antecipacaoService = saldo_antecipacaoService;
        }
    }
}