namespace hbhg.Application.Services
{
    public class ARQUIVO_PESSOAAppService : ServiceBase<ARQUIVO_PESSOAViewModel, ARQUIVO_PESSOA>, IARQUIVO_PESSOAAppService
    {
        private readonly IMapper _mapper;
