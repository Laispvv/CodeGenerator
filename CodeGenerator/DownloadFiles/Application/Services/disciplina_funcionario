¾using AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class DISCIPLINA_FUNCIONARIOAppService : ServiceBase<DISCIPLINA_FUNCIONARIOViewModel, DISCIPLINA_FUNCIONARIO>, IDISCIPLINA_FUNCIONARIOAppService
    {
        private readonly IMapper _mapper;
        private readonly IDISCIPLINA_FUNCIONARIOService _disciplina_funcionarioService;
        public DISCIPLINA_FUNCIONARIOAppService(IMapper mapper, IDISCIPLINA_FUNCIONARIOService disciplina_funcionarioService) : base(mapper, disciplina_funcionarioService)
        {
            _mapper = mapper;
            _disciplina_funcionarioService = disciplina_funcionarioService;
        }
    }
}