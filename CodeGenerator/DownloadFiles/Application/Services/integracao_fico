êusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class INTEGRACAO_FICOAppService : ServiceBase<INTEGRACAO_FICOViewModel, INTEGRACAO_FICO>, IINTEGRACAO_FICOAppService
    {
        private readonly IMapper _mapper;
        private readonly IINTEGRACAO_FICOService _integracao_ficoService;
        public INTEGRACAO_FICOAppService(IMapper mapper, IINTEGRACAO_FICOService integracao_ficoService) : base(mapper, integracao_ficoService)
        {
            _mapper = mapper;
            _integracao_ficoService = integracao_ficoService;
        }
    }
}