¾using AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class UNIDADE_ORGANIZACIONALAppService : ServiceBase<UNIDADE_ORGANIZACIONALViewModel, UNIDADE_ORGANIZACIONAL>, IUNIDADE_ORGANIZACIONALAppService
    {
        private readonly IMapper _mapper;
        private readonly IUNIDADE_ORGANIZACIONALService _unidade_organizacionalService;
        public UNIDADE_ORGANIZACIONALAppService(IMapper mapper, IUNIDADE_ORGANIZACIONALService unidade_organizacionalService) : base(mapper, unidade_organizacionalService)
        {
            _mapper = mapper;
            _unidade_organizacionalService = unidade_organizacionalService;
        }
    }
}