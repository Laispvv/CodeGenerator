öusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class CONTRATO_ADITIVOAppService : ServiceBase<CONTRATO_ADITIVOViewModel, CONTRATO_ADITIVO>, ICONTRATO_ADITIVOAppService
    {
        private readonly IMapper _mapper;
        private readonly ICONTRATO_ADITIVOService _contrato_aditivoService;
        public CONTRATO_ADITIVOAppService(IMapper mapper, ICONTRATO_ADITIVOService contrato_aditivoService) : base(mapper, contrato_aditivoService)
        {
            _mapper = mapper;
            _contrato_aditivoService = contrato_aditivoService;
        }
    }
}