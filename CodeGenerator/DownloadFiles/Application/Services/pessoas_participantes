²using AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class PESSOAS_PARTICIPANTESAppService : ServiceBase<PESSOAS_PARTICIPANTESViewModel, PESSOAS_PARTICIPANTES>, IPESSOAS_PARTICIPANTESAppService
    {
        private readonly IMapper _mapper;
        private readonly IPESSOAS_PARTICIPANTESService _pessoas_participantesService;
        public PESSOAS_PARTICIPANTESAppService(IMapper mapper, IPESSOAS_PARTICIPANTESService pessoas_participantesService) : base(mapper, pessoas_participantesService)
        {
            _mapper = mapper;
            _pessoas_participantesService = pessoas_participantesService;
        }
    }
}