–using AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class DESCONTOAppService : ServiceBase<DESCONTOViewModel, DESCONTO>, IDESCONTOAppService
    {
        private readonly IMapper _mapper;
        private readonly IDESCONTOService _descontoService;
        public DESCONTOAppService(IMapper mapper, IDESCONTOService descontoService) : base(mapper, descontoService)
        {
            _mapper = mapper;
            _descontoService = descontoService;
        }
    }
}