®using AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class CALENDARIOAppService : ServiceBase<CALENDARIOViewModel, CALENDARIO>, ICALENDARIOAppService
    {
        private readonly IMapper _mapper;
        private readonly ICALENDARIOService _calendarioService;
        public CALENDARIOAppService(IMapper mapper, ICALENDARIOService calendarioService) : base(mapper, calendarioService)
        {
            _mapper = mapper;
            _calendarioService = calendarioService;
        }
    }
}