®using AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class AREA_CURSOAppService : ServiceBase<AREA_CURSOViewModel, AREA_CURSO>, IAREA_CURSOAppService
    {
        private readonly IMapper _mapper;
        private readonly IAREA_CURSOService _area_cursoService;
        public AREA_CURSOAppService(IMapper mapper, IAREA_CURSOService area_cursoService) : base(mapper, area_cursoService)
        {
            _mapper = mapper;
            _area_cursoService = area_cursoService;
        }
    }
}