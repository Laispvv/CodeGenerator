Æusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class REQUERIMENTOAppService : ServiceBase<REQUERIMENTOViewModel, REQUERIMENTO>, IREQUERIMENTOAppService
    {
        private readonly IMapper _mapper;
        private readonly IREQUERIMENTOService _requerimentoService;
        public REQUERIMENTOAppService(IMapper mapper, IREQUERIMENTOService requerimentoService) : base(mapper, requerimentoService)
        {
            _mapper = mapper;
            _requerimentoService = requerimentoService;
        }
    }
}