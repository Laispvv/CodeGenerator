Òusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class PERFIL_ACESSOAppService : ServiceBase<PERFIL_ACESSOViewModel, PERFIL_ACESSO>, IPERFIL_ACESSOAppService
    {
        private readonly IMapper _mapper;
        private readonly IPERFIL_ACESSOService _perfil_acessoService;
        public PERFIL_ACESSOAppService(IMapper mapper, IPERFIL_ACESSOService perfil_acessoService) : base(mapper, perfil_acessoService)
        {
            _mapper = mapper;
            _perfil_acessoService = perfil_acessoService;
        }
    }
}