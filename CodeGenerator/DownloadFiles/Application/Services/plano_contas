using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class PLANO_CONTASAppService : ServiceBase<PLANO_CONTASViewModel, PLANO_CONTAS>, IPLANO_CONTASAppService
    {
        private readonly IMapper _mapper;
        private readonly IPLANO_CONTASService _plano_contasService;
        public PLANO_CONTASAppService(IMapper mapper, IPLANO_CONTASService plano_contasService) : base(mapper, plano_contasService)
        {
            _mapper = mapper;
            _plano_contasService = plano_contasService;
        }
    }
}