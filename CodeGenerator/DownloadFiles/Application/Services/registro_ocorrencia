šusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class REGISTRO_OCORRENCIAAppService : ServiceBase<REGISTRO_OCORRENCIAViewModel, REGISTRO_OCORRENCIA>, IREGISTRO_OCORRENCIAAppService
    {
        private readonly IMapper _mapper;
        private readonly IREGISTRO_OCORRENCIAService _registro_ocorrenciaService;
        public REGISTRO_OCORRENCIAAppService(IMapper mapper, IREGISTRO_OCORRENCIAService registro_ocorrenciaService) : base(mapper, registro_ocorrenciaService)
        {
            _mapper = mapper;
            _registro_ocorrenciaService = registro_ocorrenciaService;
        }
    }
}