òusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class TERMOAppService : ServiceBase<TERMOViewModel, TERMO>, ITERMOAppService
    {
        private readonly IMapper _mapper;
        private readonly ITERMOService _termoService;
        public TERMOAppService(IMapper mapper, ITERMOService termoService) : base(mapper, termoService)
        {
            _mapper = mapper;
            _termoService = termoService;
        }
    }
}