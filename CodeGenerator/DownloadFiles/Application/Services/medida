using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class MEDIDAAppService : ServiceBase<MEDIDAViewModel, MEDIDA>, IMEDIDAAppService
    {
        private readonly IMapper _mapper;
        private readonly IMEDIDAService _medidaService;
        public MEDIDAAppService(IMapper mapper, IMEDIDAService medidaService) : base(mapper, medidaService)
        {
            _mapper = mapper;
            _medidaService = medidaService;
        }
    }
}