using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class PEDIDO_PRODUTOAppService : ServiceBase<PEDIDO_PRODUTOViewModel, PEDIDO_PRODUTO>, IPEDIDO_PRODUTOAppService
    {
        private readonly IMapper _mapper;
        private readonly IPEDIDO_PRODUTOService _pedido_produtoService;
        public PEDIDO_PRODUTOAppService(IMapper mapper, IPEDIDO_PRODUTOService pedido_produtoService) : base(mapper, pedido_produtoService)
        {
            _mapper = mapper;
            _pedido_produtoService = pedido_produtoService;
        }
    }
}