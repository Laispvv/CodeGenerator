ºusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class RESPONSAVELAppService : ServiceBase<RESPONSAVELViewModel, RESPONSAVEL>, IRESPONSAVELAppService
    {
        private readonly IMapper _mapper;
        private readonly IRESPONSAVELService _responsavelService;
        public RESPONSAVELAppService(IMapper mapper, IRESPONSAVELService responsavelService) : base(mapper, responsavelService)
        {
            _mapper = mapper;
            _responsavelService = responsavelService;
        }
    }
}