using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class FREQUENCIA_OFICINAAppService : ServiceBase<FREQUENCIA_OFICINAViewModel, FREQUENCIA_OFICINA>, IFREQUENCIA_OFICINAAppService
    {
        private readonly IMapper _mapper;
        private readonly IFREQUENCIA_OFICINAService _frequencia_oficinaService;
        public FREQUENCIA_OFICINAAppService(IMapper mapper, IFREQUENCIA_OFICINAService frequencia_oficinaService) : base(mapper, frequencia_oficinaService)
        {
            _mapper = mapper;
            _frequencia_oficinaService = frequencia_oficinaService;
        }
    }
}