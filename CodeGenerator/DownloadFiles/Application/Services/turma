òusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class TURMAAppService : ServiceBase<TURMAViewModel, TURMA>, ITURMAAppService
    {
        private readonly IMapper _mapper;
        private readonly ITURMAService _turmaService;
        public TURMAAppService(IMapper mapper, ITURMAService turmaService) : base(mapper, turmaService)
        {
            _mapper = mapper;
            _turmaService = turmaService;
        }
    }
}