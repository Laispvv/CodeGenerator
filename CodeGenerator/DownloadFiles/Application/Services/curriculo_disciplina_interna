namespace hbhg.Application.Services
{
    public class CURRICULO_DISCIPLINA_INTERNAAppService : ServiceBase<CURRICULO_DISCIPLINA_INTERNAViewModel, CURRICULO_DISCIPLINA_INTERNA>, ICURRICULO_DISCIPLINA_INTERNAAppService
