öusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class USUARIO_MENSAGEMAppService : ServiceBase<USUARIO_MENSAGEMViewModel, USUARIO_MENSAGEM>, IUSUARIO_MENSAGEMAppService
    {
        private readonly IMapper _mapper;
        private readonly IUSUARIO_MENSAGEMService _usuario_mensagemService;
        public USUARIO_MENSAGEMAppService(IMapper mapper, IUSUARIO_MENSAGEMService usuario_mensagemService) : base(mapper, usuario_mensagemService)
        {
            _mapper = mapper;
            _usuario_mensagemService = usuario_mensagemService;
        }
    }
}