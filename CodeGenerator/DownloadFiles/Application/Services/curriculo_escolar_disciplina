    public class CURRICULO_ESCOLAR_DISCIPLINAAppService : ServiceBase<CURRICULO_ESCOLAR_DISCIPLINAViewModel, CURRICULO_ESCOLAR_DISCIPLINA>, ICURRICULO_ESCOLAR_DISCIPLINAAppService
