²using AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class BANCO_REMESSA_DETALHEAppService : ServiceBase<BANCO_REMESSA_DETALHEViewModel, BANCO_REMESSA_DETALHE>, IBANCO_REMESSA_DETALHEAppService
    {
        private readonly IMapper _mapper;
        private readonly IBANCO_REMESSA_DETALHEService _banco_remessa_detalheService;
        public BANCO_REMESSA_DETALHEAppService(IMapper mapper, IBANCO_REMESSA_DETALHEService banco_remessa_detalheService) : base(mapper, banco_remessa_detalheService)
        {
            _mapper = mapper;
            _banco_remessa_detalheService = banco_remessa_detalheService;
        }
    }
}