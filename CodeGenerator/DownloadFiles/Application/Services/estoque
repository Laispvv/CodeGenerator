using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class ESTOQUEAppService : ServiceBase<ESTOQUEViewModel, ESTOQUE>, IESTOQUEAppService
    {
        private readonly IMapper _mapper;
        private readonly IESTOQUEService _estoqueService;
        public ESTOQUEAppService(IMapper mapper, IESTOQUEService estoqueService) : base(mapper, estoqueService)
        {
            _mapper = mapper;
            _estoqueService = estoqueService;
        }
    }
}