öusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class ANO_LETIVO_TURMAAppService : ServiceBase<ANO_LETIVO_TURMAViewModel, ANO_LETIVO_TURMA>, IANO_LETIVO_TURMAAppService
    {
        private readonly IMapper _mapper;
        private readonly IANO_LETIVO_TURMAService _ano_letivo_turmaService;
        public ANO_LETIVO_TURMAAppService(IMapper mapper, IANO_LETIVO_TURMAService ano_letivo_turmaService) : base(mapper, ano_letivo_turmaService)
        {
            _mapper = mapper;
            _ano_letivo_turmaService = ano_letivo_turmaService;
        }
    }
}