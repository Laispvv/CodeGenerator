²using AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class INSCRICAO_CERTIFICADOAppService : ServiceBase<INSCRICAO_CERTIFICADOViewModel, INSCRICAO_CERTIFICADO>, IINSCRICAO_CERTIFICADOAppService
    {
        private readonly IMapper _mapper;
        private readonly IINSCRICAO_CERTIFICADOService _inscricao_certificadoService;
        public INSCRICAO_CERTIFICADOAppService(IMapper mapper, IINSCRICAO_CERTIFICADOService inscricao_certificadoService) : base(mapper, inscricao_certificadoService)
        {
            _mapper = mapper;
            _inscricao_certificadoService = inscricao_certificadoService;
        }
    }
}