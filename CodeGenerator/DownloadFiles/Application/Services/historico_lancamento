¦using AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class HISTORICO_LANCAMENTOAppService : ServiceBase<HISTORICO_LANCAMENTOViewModel, HISTORICO_LANCAMENTO>, IHISTORICO_LANCAMENTOAppService
    {
        private readonly IMapper _mapper;
        private readonly IHISTORICO_LANCAMENTOService _historico_lancamentoService;
        public HISTORICO_LANCAMENTOAppService(IMapper mapper, IHISTORICO_LANCAMENTOService historico_lancamentoService) : base(mapper, historico_lancamentoService)
        {
            _mapper = mapper;
            _historico_lancamentoService = historico_lancamentoService;
        }
    }
}