¢using AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class AVALIACAOAppService : ServiceBase<AVALIACAOViewModel, AVALIACAO>, IAVALIACAOAppService
    {
        private readonly IMapper _mapper;
        private readonly IAVALIACAOService _avaliacaoService;
        public AVALIACAOAppService(IMapper mapper, IAVALIACAOService avaliacaoService) : base(mapper, avaliacaoService)
        {
            _mapper = mapper;
            _avaliacaoService = avaliacaoService;
        }
    }
}