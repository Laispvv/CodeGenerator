using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class BOLETOAppService : ServiceBase<BOLETOViewModel, BOLETO>, IBOLETOAppService
    {
        private readonly IMapper _mapper;
        private readonly IBOLETOService _boletoService;
        public BOLETOAppService(IMapper mapper, IBOLETOService boletoService) : base(mapper, boletoService)
        {
            _mapper = mapper;
            _boletoService = boletoService;
        }
    }
}