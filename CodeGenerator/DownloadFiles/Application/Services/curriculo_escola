öusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class CURRICULO_ESCOLAAppService : ServiceBase<CURRICULO_ESCOLAViewModel, CURRICULO_ESCOLA>, ICURRICULO_ESCOLAAppService
    {
        private readonly IMapper _mapper;
        private readonly ICURRICULO_ESCOLAService _curriculo_escolaService;
        public CURRICULO_ESCOLAAppService(IMapper mapper, ICURRICULO_ESCOLAService curriculo_escolaService) : base(mapper, curriculo_escolaService)
        {
            _mapper = mapper;
            _curriculo_escolaService = curriculo_escolaService;
        }
    }
}