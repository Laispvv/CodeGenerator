–using AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class DEPOSITOAppService : ServiceBase<DEPOSITOViewModel, DEPOSITO>, IDEPOSITOAppService
    {
        private readonly IMapper _mapper;
        private readonly IDEPOSITOService _depositoService;
        public DEPOSITOAppService(IMapper mapper, IDEPOSITOService depositoService) : base(mapper, depositoService)
        {
            _mapper = mapper;
            _depositoService = depositoService;
        }
    }
}