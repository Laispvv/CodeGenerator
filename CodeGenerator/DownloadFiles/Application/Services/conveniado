®using AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class CONVENIADOAppService : ServiceBase<CONVENIADOViewModel, CONVENIADO>, ICONVENIADOAppService
    {
        private readonly IMapper _mapper;
        private readonly ICONVENIADOService _conveniadoService;
        public CONVENIADOAppService(IMapper mapper, ICONVENIADOService conveniadoService) : base(mapper, conveniadoService)
        {
            _mapper = mapper;
            _conveniadoService = conveniadoService;
        }
    }
}