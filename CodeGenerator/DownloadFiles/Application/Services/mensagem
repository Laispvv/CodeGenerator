–using AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class MENSAGEMAppService : ServiceBase<MENSAGEMViewModel, MENSAGEM>, IMENSAGEMAppService
    {
        private readonly IMapper _mapper;
        private readonly IMENSAGEMService _mensagemService;
        public MENSAGEMAppService(IMapper mapper, IMENSAGEMService mensagemService) : base(mapper, mensagemService)
        {
            _mapper = mapper;
            _mensagemService = mensagemService;
        }
    }
}