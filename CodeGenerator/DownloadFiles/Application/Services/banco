òusing AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class BANCOAppService : ServiceBase<BANCOViewModel, BANCO>, IBANCOAppService
    {
        private readonly IMapper _mapper;
        private readonly IBANCOService _bancoService;
        public BANCOAppService(IMapper mapper, IBANCOService bancoService) : base(mapper, bancoService)
        {
            _mapper = mapper;
            _bancoService = bancoService;
        }
    }
}