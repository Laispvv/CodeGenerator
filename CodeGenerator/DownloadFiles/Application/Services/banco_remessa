using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class BANCO_REMESSAAppService : ServiceBase<BANCO_REMESSAViewModel, BANCO_REMESSA>, IBANCO_REMESSAAppService
    {
        private readonly IMapper _mapper;
        private readonly IBANCO_REMESSAService _banco_remessaService;
        public BANCO_REMESSAAppService(IMapper mapper, IBANCO_REMESSAService banco_remessaService) : base(mapper, banco_remessaService)
        {
            _mapper = mapper;
            _banco_remessaService = banco_remessaService;
        }
    }
}