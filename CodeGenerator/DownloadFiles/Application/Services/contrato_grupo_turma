¦using AutoMapper;
using hbhg.Application.Interfaces;
using hbhg.Application.Services.Common;
using hbhg.Application.ViewModels;
using hbhg.Domain.Entities;
using hbhg.Domain.Interfaces.Service;

namespace hbhg.Application.Services
{
    public class CONTRATO_GRUPO_TURMAAppService : ServiceBase<CONTRATO_GRUPO_TURMAViewModel, CONTRATO_GRUPO_TURMA>, ICONTRATO_GRUPO_TURMAAppService
    {
        private readonly IMapper _mapper;
        private readonly ICONTRATO_GRUPO_TURMAService _contrato_grupo_turmaService;
        public CONTRATO_GRUPO_TURMAAppService(IMapper mapper, ICONTRATO_GRUPO_TURMAService contrato_grupo_turmaService) : base(mapper, contrato_grupo_turmaService)
        {
            _mapper = mapper;
            _contrato_grupo_turmaService = contrato_grupo_turmaService;
        }
    }
}